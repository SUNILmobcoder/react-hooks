 function decrementCount() {\n        dispatch({type: ACTION.DECREMENT })\n    }\n    function incrementCount() {\n        dispatch({type: ACTION.INCREMENT })\n    }\n    return (\n        <>\n            <button onClick={decrementCount}>-</button>\n            <span>{state.count}</span>\n            <button onClick={incrementCount}>+</button>\n        </>\n    )\n}\n","export default __webpack_public_path__ + \"static/media/useState.5531e614.png\";","export default __webpack_public_path__ + \"static/media/useContext.871b8e0f.png\";","export default __webpack_public_path__ + \"static/media/contextClass.d572f597.png\";","export default __webpack_public_path__ + \"static/media/contextFunction.c68854ba.png\";","export default __webpack_public_path__ + \"static/media/useReducer.30faf342.png\";","export default __webpack_public_path__ + \"static/media/useReducerTodo.d6dfd247.png\";","export default __webpack_public_path__ + \"static/media/reducerTodo.5d597fa9.png\";","export default __webpack_public_path__ + \"static/media/useCallback.288717d5.png\";","export default __webpack_public_path__ + \"static/media/useMemo.9ecb9783.png\";","export default __webpack_public_path__ + \"static/media/useRef.c65ccd81.png\";","import React from 'react';\n\nimport usestate from './pic/useState.png';\n\nimport usecontext from './pic/useContext.png';\nimport contextClass from './pic/contextClass.png';\nimport contextFunction from './pic/contextFunction.png';\n\nimport usereducer from './pic/useReducer.png';\nimport usereducertodo from './pic/useReducerTodo.png';\nimport reducertodo from './pic/reducerTodo.png';\n\nimport usecallback from './pic/useCallback.png';\nimport usememo from './pic/useMemo.png';\nimport useref from './pic/useRef.png';\n\n\n\nexport default function ImageRef() {\n    return (\n        <div>\n            <h1>useState</h1>\n            <img src={usestate} alt=\"useState\" />\n\n            <h1>useContext</h1>\n            <img src={usecontext} alt=\"useContext\" />\n